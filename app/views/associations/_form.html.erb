  <div class="span6 offset3">
    <%= form_for(@association) do |f| %>
      <%= render 'shared/error_messages' %>
     
      <%= f.label :name %>
      <%= f.text_field :name %>

      <%= f.label :info %>
      <%= f.text_area :info %>

      <%= f.label :webPage %>
      <%= f.text_field :webPage %>

      <%= f.submit "Submit", class: "btn btn-large btn-primary" %>
    <% end %>
  </div>